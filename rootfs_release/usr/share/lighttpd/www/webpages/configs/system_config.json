                "x86_64" : ""
            }
        },
        "Linux_Version" : {
            "cmd" : {
                "armv7l" : "uname -a | awk -F ' ' '{print $3}'",
                "x86_64" : "uname -a | awk -F ' ' '{print $3}'"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : "4.8.0-54-generic"
